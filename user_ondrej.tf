resource "keycloak_user" "ondrej" {
  lifecycle {
    ignore_changes = [
      required_actions,
    ]
  }

  realm_id = keycloak_realm.example.id
  username = "ondrej"
  enabled  = true

  email          = "ondrej@example.com"
  email_verified = true
  first_name     = "Ondrej"
  last_name      = "Sika"

  initial_password {
    value     = "a"
    temporary = true
  }
}
